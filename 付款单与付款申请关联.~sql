
--付款单与付款申请关联

--先找一条关联对照数据
 select CONTRACTNO, TOP_BILLID, TOP_BILLTYPE, TOP_ITEMID, TOP_TERMCH
   from ap_payitem
  where billno = '80100FK19030070'
    and dr = 0
--1	GCL\XXJC-CG(B)-2019-0546	1001D110000000G0ZPOX	36D1	1001D110000000G0ZPOY	1001B110000000016MJ6

select CONTRACTNO, TOP_BILLID, TOP_BILLTYPE, TOP_ITEMID, TOP_TERMCH --构成关联的字段
  from ap_payitem
 where billno = '80100FK19030069'
   and dr = 0
   for update
     ;
   
   
