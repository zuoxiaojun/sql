select code, name
  from bd_material
 where name in ('9800000001',
                '9800000002',
                '9800000003',
                '9800000004',
                '9800000005',
                '9800000006',
                '9800000007',
                '9800000008',
                '9800000009',
                '9800000010',
                '9800000011',
                '9800000012',
                '9800000013')
