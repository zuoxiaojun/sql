select src_syscode from ar_gatherbill where billno ='80100SK18060032' for update 


update  ar_gatherbill set dr=0 where billno ='80100SK18060032';

update ar_gatheritem
   set dr = 0
 where pk_gatherbill = (select pk_gatherbill
                          from ar_gatherbill
                         where billno = '80100SK18060032')
