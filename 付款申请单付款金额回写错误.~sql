select olcactualpaysum,
       olcorigapplysum,
       olcpaysum,
       paysum,
       pk_apply,
       glcunpaysum,
       gllcunpaysum,
       olcunpaysum,
       unpaysum
  from cmp_apply
 where vbillno = 'SS18052470'
   for update --1001D110000000DGERPA
 
 select paymny, --累计付款金额
        olcpaymny, --累计付款金额 组织本币
        gllcapplymny,
        dr,
        glcunpaymny ,
        gllcunpaymny ,
        olcunpaymny ,
        unpaymny 
   from cmp_apply_b
  where pk_apply = '1001D110000000D5Y7DD'
    for update
